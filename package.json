{
  "name": "posthtml-render",
  "version": "0.0.0",
  "description": "Render PostHTMLTree to HTML/XML",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && mocha",
    "lint": "jshint . && jscs . -v",
    "coverage": "istanbul cover --report text --report html --report lcov node_modules/mocha/bin/_mocha -- -R dot",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voischev/posthtml-render.git"
  },
  "keywords": [
    "posthtml",
    "render",
    "html",
    "xml",
    "posthtml-tree"
  ],
  "author": "Ivan Voischev <voischev.ivan@ya.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/voischev/posthtml-render/issues"
  },
  "homepage": "https://github.com/voischev/posthtml-render#readme",
  "devDependencies": {
    "chai": "^3.3.0",
    "istanbul": "^0.4.0",
    "jscs": "^2.3.4",
    "jshint": "^2.8.0",
    "mocha": "^2.3.3"
  }
}
